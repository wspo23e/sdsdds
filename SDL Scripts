local mouse = game.Players.LocalPlayer:GetMouse()
mouse.Icon = "http://www.roblox.com/asset?id=15737619870"

-----------------------------------------------------------------------------------------------------------------------------------------------------------------------
 --THIS IS THE SPIDER WEB SHOOTER

local userInputService = game:GetService("UserInputService")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local mouse = player:GetMouse()

local dragging = false
local originalGravity = workspace.Gravity

-- Function to start dragging towards mouse position
local function startDrag()
    if dragging then return end

    dragging = true
    workspace.Gravity = 0

    local mouseTarget = mouse.Hit.Position
    local direction = (mouseTarget - humanoidRootPart.Position).Unit
    local speed = 80

    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = direction * speed
    bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    bodyVelocity.Parent = humanoidRootPart

    userInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.L then
            -- Play audio when "L" is pressed
            local audio = Instance.new("Sound")
            audio.SoundId = "rbxassetid://9119460428"  -- Replace with your audio ID
            audio.Parent = player
            audio:Play()

            stopDrag()
        end
    end)
end

-- Function to stop dragging
local function stopDrag()
    if not dragging then return end

    dragging = false
    workspace.Gravity = originalGravity

    local bodyVelocity = humanoidRootPart:FindFirstChildOfClass("BodyVelocity")
    if bodyVelocity then
        bodyVelocity:Destroy()
    end
end

-- Connect the startDrag function when the script is loaded
userInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.L then
        if dragging then
            stopDrag()
        else
            startDrag()
        end
    end
end)




    -- Create a TextLabel for GUI controlls
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 700, 0, 50)
    textLabel.Position = UDim2.new(0.02, 0, 0.384, 0)  -- Adjusted position
    textLabel.AnchorPoint = Vector2.new(0.5, 0.5)
    textLabel.BackgroundColor3 = Color3.new(0, 0, 0)
    textLabel.TextColor3 = Color3.new(0, 0, 255)
    textLabel.Font = Enum.Font.SourceSans
    textLabel.TextSize = 30
    textLabel.TextWrapped = true
    textLabel.BackgroundTransparency = 1
    textLabel.TextStrokeTransparency = 0.5
    textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
    textLabel.Text = "FrontflipKey = Z"  -- Initial empty text
    textLabel.Parent = gui

