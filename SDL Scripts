print("Controlls: ")
wait(0.1)
print("SPIDER WEB SHOOT: L")
wait(0.1)
print("Frontflip = Z")
wait(0.1)
print("Backflip = X")
wait(0.1)
print("AirJump = Space")
wait(0.01)

local camera = game.Workspace.CurrentCamera

local desiredFOV = 200  

local function changeFOV()
    camera.FieldOfView = desiredFOV
end

changeFOV()

wait(0.2)

local soundId = "rbxassetid://1842802203"

-- Blur function
local function applyBlur()
    local blur = Instance.new("BlurEffect")
    blur.Size = 25  -- Adjust the blur intensity
    blur.Name = "CustomBlur"
    blur.Parent = game.Lighting
end

-- Unblur function
local function removeBlur()
    local blur = game.Lighting:FindFirstChild("CustomBlur")
    if blur then
        blur:Destroy()
    end
end

-- Function to animate text like a typewriter
local function typewriterAnimation(textLabel, text, speed)
    for i = 1, #text do
        textLabel.Text = text:sub(1, i)
        wait(speed)
    end
end

-- Play sound, apply blur, and show typewriter animation
local function executeEffect()
    local player = game.Players.LocalPlayer

    -- Create a ScreenGui
    local gui = Instance.new("ScreenGui")
    gui.Name = "TypewriterGui"
    gui.Parent = player.PlayerGui

    -- Create a TextLabel
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 700, 0, 50)
    textLabel.Position = UDim2.new(0.580, -100, 0.220, -25)  -- Adjusted position
    textLabel.AnchorPoint = Vector2.new(0.5, 0.5)
    textLabel.BackgroundColor3 = Color3.new(0, 0, 0)
    textLabel.TextColor3 = Color3.new(0, 0, 255)
    textLabel.Font = Enum.Font.SourceSans
    textLabel.TextSize = 30
    textLabel.TextWrapped = true
    textLabel.BackgroundTransparency = 1
    textLabel.TextStrokeTransparency = 0.5
    textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
    textLabel.Text = ""  -- Initial empty text
    textLabel.Parent = gui

    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Parent = player
    sound:Play()

    applyBlur()

    -- Text to display and speed of the typewriter animation
    local textToDisplay = "SpiderMan ReAnimation made by SDL, press F9 to check the controls"
    local animationSpeed = 0.05  -- Adjust the speed

    -- Trigger the typewriter animation
    typewriterAnimation(textLabel, textToDisplay, animationSpeed)

    wait(5)  -- Wait for 10 seconds

    removeBlur()

    -- Clean up the ScreenGui after the animation
    gui:Destroy()
end

-- Execute the effect when the LocalScript is loaded
executeEffect()

wait(1)

while true do
    local c = game.Players:GetChildren()
    for i = 1, #c do
        if c[i].ClassName == "Player" then
        c[i].CameraMode = "LockFirstPerson"
            end
        end
    wait(0.01)
    end

